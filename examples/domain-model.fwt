Package Application
{
	Class Person{
		name: string,
		age: number,
	}

	Class Car{
		color: string,
	}

	Class Engine{
		serialNumber: string,
	}

	Class School {
		name: string,
	}

	Class Student{
		name: string,
		age: number,
	}

	Class Vehicle{
		color: string,
	}

	Association Ownership{
		Car -- Person [* .. 1]
	}

	Agregation Enrollment{
		School -- Student [1..*]
	}

	Composition CarEngine{
		Car -> Engine [1..1]
	}

}
