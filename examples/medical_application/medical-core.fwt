@domain
Package MedicalCore {
    Class Person {
        firstName: string,
        lastName: string,
        birthDate: string,
        gender: string,
        @id socialSecurityNumber: string {notnull},
    }
    
    Class Address {
        street: string,
        city: string,
        state: string,
        zipCode: string,
    }
    
    Class MedicalRecord {
        @id recordId: number {notnull},
        createdDate: string,
        lastUpdated: string,
    }
}