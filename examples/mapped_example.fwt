//example of mapped

@domain
Package domainExample {

	@mapped
	Class ObjetoDominio {
		uuid: string {notnull},
		equals(Object): bool,
		hashCode(): number,
	}

	@mapped
	Class ObjetoPersistente{
		@id identificador: string,
		@version versao: string,
	}

	Inheritance {
		ObjetoPersistente -> ObjetoDominio
	}
}